   .bottom-margin{border-bottom: 10px solid #f2f2f2;}

   /*social links*/
   /*#footer p{width: 30%;font-size: 13px;}*/
   .copyright{width: 30%;font-size: 13px;}
   #social-links{width: 70%;}

}

