/* 
  ##Device = Low Resolution Tablets, Mobiles (Landscape)
  ##Screen = B/w 481px to 767px
*/
@media (min-width: 481px) and (max-width: 767px) {
	/*.wrapper{background-color: yellow;}*/
	.container{width: 90%;}
	.nav-container{width: 90%;}
	h3{margin-top: 10px;margin-bottom: 20px;}

	/*navigatin menu*/
	#nav-desktop{display: none;}
	#nav-mobile{display: block;}
	#navigation-icon-wrapper{margin-top: 4px;}  
	.nav-mob-menu a{display: block; padding-top: 20px;padding-bottom: 20px;width: 100%;
  						text-align: center;cursor: pointer;}
  	#first-menu{color: #fe4918;}

  	/*about me*/
  	#profile-image{width:25%;float: left;}
  	#info {width:75%;float:left;padding-left:4%;}
  	#linfo{width:50%;float: left;line-height: 43.2px;}
  	#rinfo{width:50%;float: left;line-height: 43.2px;padding-left: 8%;}
  	#info p{width: 100%;}
   
   /*experience and services*/
   .boxes-wrapper{width: 90%;}
   .box-outer{width: 100%}
   .box{width: 100%;height: 300px;}

   /*blog*/
   .blog-outer{width: 100%;padding: 5px;float: none;}
   .blog-box{width: 100%;}
   .blog-box img{width: 100%;}

   /*videos*/
   #video-thumbnails-wrapper{width: 100%;}
   #player{width: 100%;}
   .v-outer{width: 49.4%;/*height: 90px;*/}
   .v-bg-wrapper{width:100%;height: 90px;}
   .vlink{width: 64px;height: 64px;/*position: relative;top: 15px;*/}

   /*contact form*/
   .ibox {width: 100%;}
   #message{width: 100%;}
   #submit{width:100%;}

   /*contact-banner*/
   #contact-banner{width: 100%;padding: 0px;}
