	#info {width:75%;float:left;padding-left:4%;}
	#linfo{width:50%;float: left;line-height: 43.2px;}
	#rinfo{width:50%;float: left;line-height: 43.2px;}
	#info p{width: 100%;}
	
	/*experience and services*/
	.box-outer{width: 33.3%;}

	/*blog*/
	.blog-outer{width: 33.3%;padding:5px;}
	.blog-box{width:100%;}
	.blog-box img{width: 100%;}

	/*videos*/
	#player{width: 100%;}
	#video-thumbnails-wrapper{width: 100%;}
	.v-outer{width: 24%;height:height:160px;}
	.v-bg-wrapper{width: 100%;height:160px; }
	.vlink{height: 100px;width: 100px;/*position: relative;top: 33px;*/}

	/*contact-banner*/
	#contact-banner{width: 100%;padding-top: 35px;padding-bottom: 35px;}
