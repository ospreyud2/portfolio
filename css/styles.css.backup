	/*about me*/
	#profile-image{width:25%;float: left;}
	#info {width:75%;float:left;padding-left:4%;}
	#linfo{width:50%;float: left;line-height: 43.2px;}
	#rinfo{width:50%;float: left;line-height: 43.2px;}
	#info p{width: 100%;}
	
	/*experience and services*/
	.box-outer{width: 33.3%}
	.box{width: 100%;}
	.box p{font-size: 0.8rem;}
	.box h3{font-size: 1.1rem;}
	.box-icon{width: 20%;}

	/*blog*/
	.blog-outer{width: 33.3%}
	
